{
	"appId": "xyz.flowtracker.app",
	"appName": "FlowTracker",
	"webDir": "dist/public",
	"server": {
		"androidScheme": "https"
	},
	"android": {},
	"ios": {
		"scheme": "FlowTracker",
		"contentInset": "never"
	},
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 2000,
			"backgroundColor": "#9333EA",
			"showSpinner": false
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"LocalNotifications": {
			"smallIcon": "ic_stat_icon_config_sample",
			"iconColor": "#9333EA"
		}
	}
}
